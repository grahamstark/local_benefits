        <!-- massive, silly, normalisation overkill -->
        
        <table name='las'>
                <column name='code' type='CHAR' size='10' primaryKey='true' />
                <column name='name' type='CHAR' size='60' />
        </table>
        
        <table name='country'>
                <column name='name' type='CHAR' size='30' primaryKey='true'/>
        </table>
        
        <table name='government_region'>
                <!-- alternate key -->
                <column name='name' type='CHAR' size='30' required='true'/>
                <column name='frs_region_code' type='INTEGER' primaryKey='true'/>
                <unique>
                        <unique-column name='name'/>
                </unique>
        </table>
        
        <table name='las_in_region'>
                
                <column name='lacode' type='CHAR' size='10' primaryKey='true'/>
                <column name='region'  type='CHAR' size='30' />
                <column name='frs_region_code'  type='INTEGER' primaryKey='true'/>
                
                <foreign-key foreignTable="las" >
                        <reference foreign="code" local="lacode" onDelete='none' />
                </foreign-key>
                
                <foreign-key foreignTable="government_region">
                        <reference foreign="frs_region_code" local="frs_region_code"/>
                </foreign-key>
                <foreign-key foreignTable="government_region">
                        <reference foreign="name" local="region"/>
                </foreign-key>
        </table>
        
        <table name='regions_in_country'>
                <column name='country_name' type='CHAR' size='30' primaryKey='true'/>
                <column name='region_name' type='CHAR' size='30' />
                <column name='frs_region_code'  type='INTEGER' primaryKey='true'/>
                <foreign-key foreignTable="government_region">
                        <reference foreign="frs_region_code" local="frs_region_code"/>
                </foreign-key>
                <foreign-key foreignTable="country">
                        <reference foreign="name" local="country_name"/>
                </foreign-key>
        </table>
        

</database>

